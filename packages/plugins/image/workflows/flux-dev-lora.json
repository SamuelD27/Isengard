{
  "3": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "flux1-dev.safetensors"
    }
  },
  "4": {
    "class_type": "LoraLoader",
    "inputs": {
      "lora_name": "{{LORA_PATH}}",
      "strength_model": {{LORA_STRENGTH}},
      "strength_clip": {{LORA_STRENGTH}},
      "model": ["3", 0],
      "clip": ["3", 1]
    }
  },
  "5": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": {{WIDTH}},
      "height": {{HEIGHT}},
      "batch_size": 1
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{PROMPT}}",
      "clip": ["4", 1]
    }
  },
  "7": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{NEGATIVE_PROMPT}}",
      "clip": ["4", 1]
    }
  },
  "8": {
    "class_type": "KSampler",
    "inputs": {
      "seed": {{SEED}},
      "steps": {{STEPS}},
      "cfg": {{CFG}},
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["4", 0],
      "positive": ["6", 0],
      "negative": ["7", 0],
      "latent_image": ["5", 0]
    }
  },
  "9": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["8", 0],
      "vae": ["3", 2]
    }
  },
  "10": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "isengard_output",
      "images": ["9", 0]
    }
  }
}
